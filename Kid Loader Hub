local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Kavo-Ui/main/Darkrai%20Ui", true))()

local Library = DarkraiX:Window("Kid Loader Hub","","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Welcome")
Tab2 = Library:Tab("Olekid Hub")
Tab3 = Library:Tab("VHub")
Tab4 = Library:Tab("Coco Hub") 

Tab1:Button("Welcom to Kid Loader Hub",function()
    print("Welcome")
end)

Tab2:Button("Olekid With Key System",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoeasyToMakeHubRoblox/kija/main/Key%20System"))()
end)


Tab3:Button("VHub New",function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/Veincx5315/Created/VHub/Simple'),true))()
end)

Tab4 = Library:Tab("Coco Hub")

Tab4:Button("coco",function()
    loadstring(game:HttpGet(('https://gitlab.com/cococc/cocohub/-/raw/master/CocoLoader'),true))()
end)

Tab1:Line()

--]]

DarkraiX:ToggleUi()
